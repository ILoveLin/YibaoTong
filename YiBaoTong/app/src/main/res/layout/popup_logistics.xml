<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/viewout1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/padding_0"
        android:layout_weight="1" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/padding_18"
            android:background="@drawable/shape_pop_logistics_logo_out"
            android:src="@mipmap/logo_pop_logistics" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_55"
            android:background="@drawable/shape_pop_logistics"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_12"
                android:layout_marginTop="@dimen/padding_45"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/padding_10"
                    android:paddingLeft="@dimen/padding_10"
                    android:paddingTop="@dimen/padding_10"
                    android:text="@string/logistics_resource"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_font_15" />

                <TextView
                    android:id="@+id/pop_wuliu_company"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/padding_10"
                    android:paddingTop="@dimen/padding_10"
                    android:text="EMS"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/text_font_17" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_12"
                android:layout_marginTop="@dimen/padding_6"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/padding_10"
                    android:paddingLeft="@dimen/padding_10"
                    android:paddingTop="@dimen/padding_10"
                    android:text="@string/logistics_num"
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/text_font_15" />

                <TextView
                    android:id="@+id/pop_logistics_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/padding_10"
                    android:paddingTop="@dimen/padding_10"
                    android:text="BZ1234567890786"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/text_font_17" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_12"
                android:layout_marginTop="@dimen/padding_20"
                android:drawableLeft="@mipmap/icon_yuding_tish"
                android:drawablePadding="@dimen/padding_4"
                android:gravity="center"
                android:paddingBottom="@dimen/padding_5"
                android:paddingLeft="@dimen/padding_10"
                android:paddingTop="@dimen/padding_6"
                android:text="@string/logistics_prompt"
                android:textColor="#f55959"
                android:textSize="@dimen/text_font_13" />

            <TextView
                android:id="@+id/pop_logistics_copy"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_43"
                android:layout_marginBottom="@dimen/padding_15"
                android:layout_marginLeft="@dimen/padding_22"
                android:layout_marginRight="@dimen/padding_22"
                android:background="@drawable/shape_pop_logistics_copy"
                android:gravity="center"
                android:text="@string/logistics_button"
                android:textColor="@color/color_ffffff"
                android:textSize="@dimen/text_font_17" />
        </LinearLayout>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/padding_20"
            android:background="@drawable/shape_pop_logistics_logo"
            android:src="@mipmap/logo_pop_logistics" />


    </RelativeLayout>

    <View
        android:id="@+id/viewout2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/padding_0"
        android:layout_weight="1" />
</LinearLayout>