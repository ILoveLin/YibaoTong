<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linear_pop_hospital"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/padding_5"
    android:layout_marginRight="@dimen/padding_5"
    android:background="@drawable/shape_results_pop_top"
    android:padding="1px"
    android:visibility="gone"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linear_pop_sh_one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_pop_sh_top_results"
        android:clickable="true"
        android:onClick="onBtnClick"
        android:orientation="horizontal">
        <View
            android:id="@+id/view_pop_nayao_one"
            android:layout_width="@dimen/padding_2"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/padding_18"
            android:layout_marginLeft="@dimen/padding_10"
            android:layout_marginTop="@dimen/padding_18"
            android:background="@color/color_6074b4"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/tv_pop_sh_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_15"
            android:paddingLeft="@dimen/padding_5"
            android:paddingTop="@dimen/padding_15"
            android:text="@string/sresult_hospital_name1"
            android:textColor="@color/color_444444"
            android:textSize="@dimen/text_font_14" />
        <TextView
            android:id="@+id/tv_pop_sh_one2"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_15"
            android:paddingTop="@dimen/padding_15"
            android:text="@string/sresult_hospital_name2"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/text_font_14" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/pop_sh_linear_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_pop_sh_bottom_results"
        android:clickable="true"
        android:onClick="onBtnClick"
        android:orientation="horizontal">

        <View
            android:id="@+id/view_pop_sh_linear_two"
            android:layout_width="@dimen/padding_2"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/padding_18"
            android:layout_marginLeft="@dimen/padding_10"
            android:layout_marginTop="@dimen/padding_18"
            android:background="@color/color_6074b4"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/pop_sh_tv_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_15"
            android:paddingLeft="@dimen/padding_5"
            android:paddingTop="@dimen/padding_15"
            android:text="@string/sresult_hospital_item_name1"
            android:textColor="@color/color_444444"
            android:textSize="@dimen/text_font_14" />

        <TextView
            android:id="@+id/pop_sh_tv_two2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_15"
            android:paddingTop="@dimen/padding_15"
            android:text="@string/sresult_hospital_item_name2"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/text_font_14" />
    </LinearLayout>

</LinearLayout>
